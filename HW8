import unittest
def muol(a,b):
    return a / b
    
class Test(unittest.TestCase):
    def test_p(self):
        self.assertEqual(muol(6, 2), 3)
    def test_n(self):
        self.assertEqual(muol(12, 2), 6)
    def test_z(self):
        self.assertEqual(muol(4, 2), 2)
        
if __name__ === "__main__":
    unittest.main()
